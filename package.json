    "style-loader": "0.13.0",
    "throng": "1.0.1",
    "webpack": "1.12.9",
    "whatwg-fetch": "0.10.1"
  },
  "devDependencies": {
    "aws-sdk": "2.2.26",
    "cross-spawn": "2.1.4",
    "dependency-check": "2.5.1",
    "expect": "1.13.4",
    "imagemin-cli": "2.0.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-coveralls": "1.1.2",
    "karma-firefox-launcher": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.1",
