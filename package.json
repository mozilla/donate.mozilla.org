{
  "name": "donate.mozilla.org",
  "version": "0.0.1",
  "description": "Mozilla's Donation Forms",
  "main": "start.js",
  "scripts": {
    "start": "npm-run-all build:* && npm-run-all --parallel watch:js server",
    "build:l10njson": "pontoon-to-json --dest ./public",
    "build:exchangerates": "node scripts/exchangerates.js",
    "build:babel": "babel src --compact=true -s --out-dir dist",
    "build:webpack": "webpack --optimize-minimize",
    "watch:js": "npm-run-all --parallel watch:babel watch:webpack",
    "watch:babel": "npm run build:babel -- -w",
    "watch:webpack": "webpack --progress --profile --colors --watch",
    "server": "node start.js",
    "test": "npm-run-all build:* && npm-run-all --parallel test:http test:depcheck test:lint test:karma",
    "test:karma": "karma start",
    "test:coverage": "KARMA_ENV=coverage karma start --reporters coverage,coveralls",
    "watch:test": "karma start --no-single-run --browsers Firefox",
    "test:http": "DISABLE_LOGGING=true mocha tests/http/*.test.js",
    "test:selenium": "node tests/selenium/start.js",
    "test:depcheck": "dependency-check ./package.json",
    "test:lint": "eslint --ext .js,.jsx .",
    "imagemin": "imagemin assets/images/* assets/images",
    "heroku-postbuild": "npm-run-all build:*",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla/donate.mozilla.org.git"
  },
  "keywords": [
    "donations",
    "forms",
    "mozilla"
  ],
  "author": "Mozilla",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/donate.mozilla.org/issues"
  },
  "homepage": "https://github.com/mozilla/donate.mozilla.org#readme",
  "dependencies": {
    "accept-language-parser": "1.0.2",
    "assets-webpack-plugin": "3.2.0",
    "async": "1.5.1",
    "aws-sdk": "^2.281.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.11.1",
    "bestlang": "0.0.1",
    "blankie": "4.0.0",
    "boom": "7.1.1",
    "css-loader": "0.28.4",
    "enhanced-require": "0.5.0-beta6",
    "enzyme": "2.7.0",
    "eslint": "4.11.0",
    "eslint-loader": "1.1.1",
    "eslint-plugin-react": "3.14.0",
    "exports-loader": "0.6.2",
    "extract-text-webpack-plugin": "3.0.2",
    "good": "8.0.0-rc1",
    "good-console": "^7.0.0",
    "good-console-logfmt": "1.0.3",
    "good-squeeze": "^5.0.2",
    "habitat": "3.1.2",
    "hapi": "17.2.0",
    "hapi-auth-bearer-token": "6.0.1",
    "hatchet": "0.3.2",
    "imports-loader": "0.6.5",
    "inert": "5.1.0",
    "iron": "5.0.4",
    "joi": "13.1.0",
    "json-loader": "0.5.4",
    "langmap": "0.0.12",
    "less": "2.5.3",
    "less-loader": "4.0.6",
    "npm-run-all": "1.4.0",
    "object-assign": "4.1.0",
    "on-build-webpack": "0.1.0",
    "polyfill-service": "^3.25.1",
    "pontoon-to-json": "1.0.1",
    "properties-parser": "0.3.1",
    "react": "15.2.1",
    "react-addons-test-utils": "15.2.1",
    "react-dom": "15.2.1",
    "react-ga": "2.1.1",
    "react-intl": "2.1.3",
    "react-redux": "4.4.5",
    "react-router": "2.0.0",
    "redux": "3.6.0",
    "request": "^2.87.0",
    "scooter": "5.0.0",
    "snyk": "^1.89.0",
    "stripe": "5.1.1",
    "style-loader": "0.18.2",
    "throng": "4.0.0",
    "webpack": "3.2.0",
    "whatwg-fetch": "0.10.1"
  },
  "devDependencies": {
    "cross-spawn": "2.1.4",
    "dependency-check": "2.9.1",
    "expect": "1.13.4",
    "geckodriver": "1.1.2",
    "imagemin-cli": "2.0.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "^1.1.2",
    "karma-coveralls": "1.1.2",
    "karma-firefox-launcher": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.1",
    "mocha": "2.3.4",
    "moment": "^2.22.2",
    "selenium-webdriver": "^3.6.0",
    "should": "8.0.2"
  },
  "engines": {
    "node": "^8.9.0"
  },
  "snyk": true
}
