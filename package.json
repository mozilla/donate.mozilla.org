{
  "name": "donate.mozilla.org",
  "version": "0.0.1",
  "description": "Mozilla's Donation Forms",
  "main": "start.js",
  "scripts": {
    "start": "node scripts/build",
    "start:prod": "npm-run-all build:* server",
    "start:dev": "npm-run-all --parallel build:l10njson build:exchangerates watch:js server",
    "postinstall": "npm-run-all build:*",
    "build:exchangerates": "node scripts/exchangerates.js",
    "build:l10njson": "node scripts/properties2json",
    "build:js": "webpack --optimize-minimize --optimize-dedupe",
    "watch:js": "webpack -w",
    "server": "node start.js",
    "test": "npm-run-all --parallel test:server test:http test:depcheck test:lint test:karma",
    "test:karma": "karma start",
    "test:coverage": "KARMA_ENV=coverage karma start --reporters coverage,coveralls",
    "watch:test": "karma start --no-single-run --browsers Firefox",
    "test:http": "mocha tests/http/*.test.js",
    "test:server": "node tests/start.js",
    "test:selenium": "node tests/selenium/start.js",
    "test:depcheck": "dependency-check ./package.json",
    "test:lint": "eslint --ext .js,.jsx .",
    "imagemin": "imagemin assets/images/* assets/images"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla/donate.mozilla.org.git"
  },
  "keywords": [
    "donations",
    "forms",
    "mozilla"
  ],
  "author": "Mozilla",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/donate.mozilla.org/issues"
  },
  "homepage": "https://github.com/mozilla/donate.mozilla.org#readme",
  "dependencies": {
    "assets-webpack-plugin": "3.2.0",
    "async": "1.5.1",
    "babel": "6.3.26",
    "babel-core": "6.3.26",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "blankie": "1.1.1",
    "boom": "3.1.1",
    "css-loader": "0.23.1",
    "enhanced-require": "0.5.0-beta6",
    "eslint": "1.10.3",
    "eslint-loader": "1.1.1",
    "eslint-plugin-react": "3.14.0",
    "exports-loader": "0.6.2",
    "extract-text-webpack-plugin": "0.9.1",
    "fs-writefile-promise": "1.0.3",
    "good": "6.4.0",
    "good-console-logfmt": "1.0.2",
    "habitat": "3.1.2",
    "hapi": "11.1.4",
    "hatchet": "0.3.2",
    "hoek": "3.0.4",
    "imports-loader": "0.6.5",
    "inert": "3.2.0",
    "joi": "7.1.0",
    "json-loader": "0.5.4",
    "less": "2.5.3",
    "less-loader": "2.2.2",
    "newrelic": "1.24.0",
    "npm-run-all": "1.4.0",
    "polyfill-service": "2.2.0",
    "properties-parser": "0.3.1",
    "q-io": "1.13.2",
    "react": "0.13.3",
    "react-ga": "1.2.0",
    "react-intl": "1.2.2",
    "react-router": "0.13.4",
    "request": "2.67.0",
    "scooter": "2.0.1",
    "stripe": "4.1.0",
    "style-loader": "0.13.0",
    "throng": "1.0.1",
    "webpack": "1.12.9",
    "whatwg-fetch": "0.10.1"
  },
  "devDependencies": {
    "aws-sdk": "2.2.26",
    "cross-spawn": "2.1.4",
    "dependency-check": "2.5.1",
    "expect": "1.13.4",
    "imagemin-cli": "2.0.0",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-coveralls": "1.1.2",
    "karma-firefox-launcher": "0.1.7",
    "karma-mocha": "0.2.1",
    "karma-mocha-reporter": "1.1.5",
    "karma-sourcemap-loader": "0.3.6",
    "karma-webpack": "1.7.0",
    "mocha": "2.4.5",
    "react-test-context": "0.1.0",
    "selenium-webdriver": "2.48.2",
    "should": "8.0.2"
  },
  "engines": {
    "node": "4.2.3"
  }
}
